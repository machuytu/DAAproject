
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Cổng thông tin đào tạo &mdash; UIT</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Muli:300,400,700,900" rel="stylesheet">
    <link rel="stylesheet" href="~/Assets/Home/fonts/icomoon/style.css">
    <link rel="stylesheet" href="~/Assets/Home/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Assets/Home/css/jquery-ui.css">
    <link rel="stylesheet" href="~/Assets/Home/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Assets/Home/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Assets/Home/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Assets/Home/css/jquery.fancybox.min.css">
    <link rel="stylesheet" href="~/Assets/Home/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="~/Assets/Home/fonts/flaticon/font/flaticon.css">
    <link rel="stylesheet" href="~/Assets/Home/css/nhutstyle.css" />
    <link rel="stylesheet" href="~/Assets/Home/css/aos.css">
    <link href="~/Assets/Home/css/jquery.mb.YTPlayer.min.css" media="all" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="~/Assets/Home/css/style.css">
    <link href="~/Assets/Home/css/table1.css" rel="stylesheet" />
    <link href="~/Assets/Home/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
</head>
<body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">
    <div class="site-wrap">
        <div class="site-mobile-menu site-navbar-target">
            <div class="site-mobile-menu-header">
                <div class="site-mobile-menu-close mt-3">
                    <span class="icon-close2 js-menu-toggle"></span>
                </div>
            </div>
            <div class="site-mobile-menu-body"></div>
        </div>

        <div class="py-2 bg-light">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-9 d-none d-lg-block">
                        <a href="#" class="small mr-3"><span class="icon-phone2 mr-2"></span> 0939013911</a>
                        <a href="#" class="small mr-3"><span class="icon-envelope-o mr-2"></span> nhutnm.uit@gmail.com</a>
                    </div>
                    <div class="col-lg-3 text-right">
                        @{
                            var session = (UserLogin)Session[ProjectDAA1.Common.CommonConstants.USER_SESSION];
                            if (session == null)
                            {
                                <a href="/login" class="small btn nhutbtn btn-primary px-4 py-2"><span class="icon-users"></span> Đăng Nhập</a>
                            }
                            else
                            {
                                <a href="@Url.Action("Logout", "Login")" class="small btn btn-primary px-4 py-2 nhutbtn"><span class="icon-unlock-alt"></span> Đăng Xuất</a>
                            }
                        }

                    </div>

                </div>
            </div>
        </div>

        @{Html.RenderAction("Header", "Home");}
        @RenderBody()
        
    <!-- .site-wrap -->
    <!-- loader -->
    <div id="loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#51be78" /></svg></div>
    <script src="~/Assets/Home/js/jquery-3.3.1.min.js"></script>
    <script src="~/Assets/Home/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/Assets/Home/js/jquery-ui.js"></script>
    <script src="~/Assets/Home/js/popper.min.js"></script>
    <script src="~/Assets/Home/js/bootstrap.min.js"></script>
    <script src="~/Assets/Home/js/owl.carousel.min.js"></script>
    <script src="~/Assets/Home/js/jquery.stellar.min.js"></script>
    <script src="~/Assets/Home/js/jquery.countdown.min.js"></script>
    <script src="~/Assets/Home/js/bootstrap-datepicker.min.js"></script>
    <script src="~/Assets/Home/js/jquery.easing.1.3.js"></script>
    <script src="~/Assets/Home/js/aos.js"></script>
    <script src="~/Assets/Home/js/jquery.fancybox.min.js"></script>
    <script src="~/Assets/Home/js/jquery.sticky.js"></script>
    <script src="~/Assets/Home/js/jquery.mb.YTPlayer.min.js"></script>
    <script src="~/Assets/Home/js/xuly.js"></script>
    @RenderSection("Scripts", false)
    <script src="~/Assets/Home/js/main.js"></script>
</body>
</html>